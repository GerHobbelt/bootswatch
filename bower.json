{
  "name": "bootswatch",
  "description": "Bootswatch is a collection of themes for Bootstrap.",
  "version": "3.4.0",
  "author": "Thomas Park",
  "homepage": "http://bootswatch.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/aguegu/bootswatch.git"
  },
  "license": "MIT",
  "ignore": [    
    "assets",
